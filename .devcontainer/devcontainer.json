{
  "image": "braughtg/vnc-novnc-codespace-base:1.0.0",  
  "forwardPorts": [5901, 6901],
  "portsAttributes": {
    "5901": {
      "label": "VNC"
    },
    "6901": {
      "label": "NoVNC"
    }
  },
  "remoteUser": "student",
  "workspaceFolder": "/home/student",
  "postCreateCommand": "gh codespace ports visibility 6901:public -c $CODESPACE_NAME",
  "postStartCommand": "bash /home/student/.contconf/startup.bash",
}
